#!/bin/bash

#PBS -N xx18
#PBS -l select=1
#PBS -l walltime=00:20:00
#PBS -A z01-csetds

export PBS_O_WORKDIR=$(readlink -f $PBS_O_WORKDIR)
cd $PBS_O_WORKDIR

module use $HOME/modulefiles
module load parafem

mkdir -p parafem
aprun -n 24 xx18 xx18_small &> log
cp -p log *.res parafem

mkdir -p petsc
aprun -n 24 xx18 xx18_small petsc &> log
cp -p log *.res petsc
